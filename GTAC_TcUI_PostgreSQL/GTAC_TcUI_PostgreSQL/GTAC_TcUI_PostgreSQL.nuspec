<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
    <id>GTAC_TcUI_PostgreSQL</id>
    <version>0.0.2.18</version>
    <title>$Sodecia GTAC TcUI PostgreSQL Server Extension$</title>
    <authors>Brent Lekx-Toniolo, Chris Drees</authors>
    <owners>Sodeica GTAC</owners>
    <projectUrl>http://www.tbd.tld</projectUrl>
    <license type="file">LICENSE.txt</license>
    <icon>images/logo.png</icon>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>TcHmi Server Extension for interacting with a PostgreSQL database</description>
    <releaseNotes>Alpha Release during IWS -> TcHmi migration project.</releaseNotes>
    <copyright>Copyright 2022</copyright>
    <tags>TwinCAT HMI Server</tags>
    <dependencies>
      <group targetFramework="net5.0">
        <dependency id="Npgsql" version="6.0.4" exclude="Build,Analyzers" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <!--
    target := win-x64, win-x86, etc.
    -->
    <file src="LICENSE.txt" target="" />
    <file src="bin\Debug\**\GTAC_TcUI_PostgreSQL.dll" target="runtimes\any\native\"/>
    <file src="bin\Debug\**\GTAC_TcUI_PostgreSQL.*.json" target="runtimes\any\native\"/>
    <file src="bin\Debug\**\Npgsql.dll" target="runtimes\any\native\"/>

	  <file src="images\logo.png" target="images\" />
  </files>
</package>
